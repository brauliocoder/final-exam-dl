<p id="notice"><%= notice %></p>

<h1>Memberships</h1>

<table>
  <thead>
    <tr>
      <th>Expiration</th>
      <th>Autorenewal</th>
      <th>Is active</th>
      <th>Role</th>
      <th>User</th>
      <th>Channel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @memberships.each do |membership| %>
      <tr>
        <td><%= membership.expiration %></td>
        <td><%= membership.autorenewal %></td>
        <td><%= membership.is_active %></td>
        <td><%= membership.role_id %></td>
        <td><%= membership.user_id %></td>
        <td><%= membership.channel_id %></td>
        <td><%= link_to 'Show', membership %></td>
        <td><%= link_to 'Edit', edit_membership_path(membership) %></td>
        <td><%= link_to 'Destroy', membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Membership', new_membership_path %>
