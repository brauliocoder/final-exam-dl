<% current_user.headquarters.each do |headquarter| %>
  <h5>
    Channels from <%= headquarter.brand %>
  </h5>
  <div class="row row-cols-4 g-2 mb-5">
    <% headquarter.channels.each do |channel| %>
      <div class="col">
        <div class="card border border-secondary shadow-sm p-0">
          <div class="card-body d-flex justify-content-between align-items-center">
            <%= link_to "Go", cashbox_mode_path(channel.id), class:"btn btn-outline-success border-secondary shadow-sm px-4" %>
            <span class="text-end">
              <small>
                <%= channel.name %> of <%= headquarter.brand %>
              </small>
            </span>
          </div>
        </div>
      </div>
    <% end %>    
  </div>
<% end %>