<p id="notice"><%= notice %></p>

<h1>Headquarters</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Is active</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @headquarters.each do |headquarter| %>
      <tr>
        <td><%= headquarter.brand %></td>
        <td><%= headquarter.is_active %></td>
        <td><%= headquarter.user_id %></td>
        <td><%= link_to 'Show', headquarter %></td>
        <td><%= link_to 'Edit', edit_headquarter_path(headquarter) %></td>
        <td><%= link_to 'Destroy', headquarter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Headquarter', new_headquarter_path %>
